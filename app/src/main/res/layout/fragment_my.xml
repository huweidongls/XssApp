<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#EEEEEE">
    
    <RelativeLayout
        android:background="@mipmap/my_bg"
        android:layout_width="match_parent"
        android:layout_height="183dp">

        <ImageView
            android:layout_marginLeft="7dp"
            android:layout_marginTop="12dp"
            android:src="@mipmap/my_set"
            android:layout_width="22dp"
            android:layout_height="22dp" />

        <RelativeLayout
            android:id="@+id/rl_message"
            android:layout_alignParentRight="true"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:layout_width="21dp"
            android:layout_height="25dp">

            <ImageView
                android:layout_alignParentBottom="true"
                android:src="@mipmap/my_msg"
                android:layout_width="21dp"
                android:layout_height="22dp" />

            <ImageView
                android:layout_alignParentRight="true"
                android:src="@mipmap/msg_red"
                android:layout_width="10dp"
                android:layout_height="10dp" />

        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:layout_marginTop="7dp"
        android:background="@color/white_ffffff"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/rl_fast_order"
            android:background="@drawable/selector_fragment_my_bg"
            android:layout_width="match_parent"
            android:layout_height="52dp">

            <ImageView
                android:id="@+id/iv1"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/my_rl1"
                android:layout_width="19dp"
                android:layout_height="19dp" />

            <TextView
                android:textSize="17sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/iv1"
                android:textColor="@color/black_333333"
                android:text="快速下单"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/right1"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@mipmap/right_gray"
                android:layout_width="11dp"
                android:layout_height="22dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_marginRight="8dp"
                android:layout_toLeftOf="@+id/right1"
                android:textColor="@color/black_333333"
                android:textSize="13sp"
                android:text="在这里可以直接预约下单啦"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>

        <View
            android:background="#EEEEEE"
            android:layout_marginLeft="2dp"
            android:layout_width="match_parent"
            android:layout_height="2dp"/>

        <RelativeLayout
            android:background="@drawable/selector_fragment_my_bg"
            android:layout_width="match_parent"
            android:layout_height="52dp">

            <ImageView
                android:id="@+id/iv2"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/my_rl2"
                android:layout_width="19dp"
                android:layout_height="19dp" />

            <TextView
                android:textSize="17sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/iv2"
                android:textColor="@color/black_333333"
                android:text="服务地址"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/right2"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@mipmap/right_gray"
                android:layout_width="11dp"
                android:layout_height="22dp" />

        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="7dp"
        android:background="@color/white_ffffff"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:background="@drawable/selector_fragment_my_bg"
            android:layout_width="match_parent"
            android:layout_height="52dp">

            <ImageView
                android:id="@+id/iv3"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/my_rl3"
                android:layout_width="19dp"
                android:layout_height="19dp" />

            <TextView
                android:textSize="17sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/iv3"
                android:textColor="@color/black_333333"
                android:text="我的消费记录"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/right3"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@mipmap/right_gray"
                android:layout_width="11dp"
                android:layout_height="22dp" />

        </RelativeLayout>

        <View
            android:background="#EEEEEE"
            android:layout_marginLeft="2dp"
            android:layout_width="match_parent"
            android:layout_height="2dp"/>

        <RelativeLayout
            android:id="@+id/rl4"
            android:background="@drawable/selector_fragment_my_bg"
            android:layout_width="match_parent"
            android:layout_height="52dp">

            <ImageView
                android:id="@+id/iv4"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/my_rl4"
                android:layout_width="19dp"
                android:layout_height="19dp" />

            <TextView
                android:textSize="17sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/iv4"
                android:textColor="@color/black_333333"
                android:text="我的优惠券"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/right4"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@mipmap/right_gray"
                android:layout_width="11dp"
                android:layout_height="22dp" />
            
            <RelativeLayout
                android:layout_marginRight="8dp"
                android:layout_toLeftOf="@+id/right4"
                android:id="@+id/rl_coupons_num"
                android:layout_width="100dp"
                android:layout_height="match_parent">



            </RelativeLayout>

        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="7dp"
        android:background="@color/white_ffffff"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:background="@drawable/selector_fragment_my_bg"
            android:layout_width="match_parent"
            android:layout_height="52dp">

            <ImageView
                android:id="@+id/iv5"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/my_rl5"
                android:layout_width="19dp"
                android:layout_height="19dp" />

            <TextView
                android:textSize="17sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/iv5"
                android:textColor="@color/black_333333"
                android:text="我的评价"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/right5"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@mipmap/right_gray"
                android:layout_width="11dp"
                android:layout_height="22dp" />

        </RelativeLayout>

        <View
            android:background="#EEEEEE"
            android:layout_marginLeft="2dp"
            android:layout_width="match_parent"
            android:layout_height="2dp"/>

        <RelativeLayout
            android:background="@drawable/selector_fragment_my_bg"
            android:layout_width="match_parent"
            android:layout_height="52dp">

            <ImageView
                android:id="@+id/iv6"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:src="@mipmap/my_rl6"
                android:layout_width="19dp"
                android:layout_height="19dp" />

            <TextView
                android:textSize="17sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/iv6"
                android:textColor="@color/black_333333"
                android:text="联系客服"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <ImageView
                android:id="@+id/right6"
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@mipmap/right_gray"
                android:layout_width="11dp"
                android:layout_height="22dp" />

            <TextView
                android:layout_centerVertical="true"
                android:layout_marginRight="8dp"
                android:layout_toLeftOf="@+id/right6"
                android:textColor="@color/black_333333"
                android:textSize="13sp"
                android:text="400-1234-5678"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>

    </LinearLayout>

</LinearLayout>
